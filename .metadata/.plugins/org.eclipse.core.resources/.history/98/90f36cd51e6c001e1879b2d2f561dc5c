package com.example.learningSpringFrameWork;

import org.springframework.context.annotation.AnnotationConfigApplicationContext;

public class GamingAppSpring {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		AnnotationConfigApplicationContext context = new AnnotationConfigApplicationContext(GamingConfiguration.class);
		
		context.getBean(GamingConsole.class).up();
		
		context.getBean(GamingConsole.class).left();
		
		
	}

}

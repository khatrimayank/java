package com.basics.java;

public class MultiplicationTable {
	
	void table() {
		
		for (int i=1;i<=10;i++) {
			
			System.out.printf("%d * %d = %d",5,i,5*i).println();
			
		}
		
	}
	
	public static void main(String[] args) {
		
		MultiplicationTable  printTable =new MultiplicationTable();
		
		printTable.table();
	
}
